            = ;
              = ;
        
    }
    
    
    
    //char line[OD, Coliform, pH, DOB, Nitrate, Phosphate, Turbinity, Solids, Temperature];
   // chr column[q, ] //delete this line
        
        
//Parameter equations

//classification index

    int iqa_class(void){
    double od, coliform, ph, dob, nitrate, phosphate, turbinity, solids, temp, qod, qcoliform, qph, qbod, qnitrate, qphosphate, qturbinity, qsolids, qtemp, qi_w;
     char a[8]= "Excellent",
          b[8]= "Good",
          c[8]="Reasonable",
          d[8]="Bad",
          e[8]="Unnaceptable";
        
        
        //Coliforms
        
        if (coliform > 10000) {
            qcoliform = 3;
            
            scanf("%d", &coliform);
            printf("Coliform concentration is too high. \n");
            
        } else {
            qcoliform = (-8.723*(log10(coliform)) + 88.714);
                
    }
         
       
        //pH
        
        if (ph < 2) {
            qph = 2;
            
        } else if (ph > 12) {
            qph = 3;
            
        } else if (ph > 2 && <= 12) {
                qph = 93 * exp(- pow(ph-7.5, 2))/2 * (pow (0.652,2));
               
    }
                
          
        
        //DOB
        if (dob > 30) {
            qdob = 2;
            
        } else {
            qdob = (-30.1) * (log10(dob)) + 103.45;
            
    }
    

    
      //nitrate
      if(nitrate > 100) {
          qnitrate = 1;
          
      } else {
          qnitrate = (20.8) * (log10(nitrate)) + 93.092;
    
    }
     
      //phosphate
        
      if(phosphate > 10) {
          qnitrate = 2;
          
      }else {
          qnitrate = (20.8) * (log10(nitrate) + 93.092;
                
      }
            
     //turbinity
                               
      if(turbinity > 100){
          qturbinity = 5;
              
      }else {
          qturbinity = (-26.45) * (log10(turbinity) + 136.37;
                                   
                                   
     //solids
 
     if(solids > 500) {
         qsolids = 20;
              
     }else {
         qsolids = 80 * exp(- pow(solids-50,2))/2 * (pow(0.003,2));
                            
                            
      }
    
      //temperature                         
        
        
        
    
        //IQA computation
           
          
          
           
          iqa_index = qod * qcoliform * qph * qdob * qnitrate * qphosphate * qturbinity * qsolids * qtemp;

          if(iqa_index <= 100 && iqa_index > 80){
              printf("IQA: %f Water Quality:%s\n", iqa_index, a);
          }
          if(iqa_index <= 80 && iqa_index > 50){
              printf("IQA: %f Water Quality:%s\n", iqa_index, b);
          }
          if(iqa_index <= 50 && iqa_index > 36){
              printf("IQA: %f Water Quality:%s\n", iqa_index, c);
          }
          if(iqa_index <= 36 && iqa_index > 20){
              printf("IQA: %f Water Quality:%s\n", iqa_index, d);
          }
          if(iqa_index <= 20){
              printf("IQA: %f Water Quality:%s\n", iqa_index, e);
          }
      return 0;
      
      }
 

 
