    if (fpReadFile ==0) { //check if the file is opened otherwise error
        printf("Error: The file can't be read\n");
        exit(1);
     
    
int main() {
    
    struct parameters_label {
        
        char Od[Parameters];
        char Coliforms[Parameters];
        char ph[Parameters];
        char dob[Parameters];
        char nitrate[Parameters];
        char phosphate[Parameters];
